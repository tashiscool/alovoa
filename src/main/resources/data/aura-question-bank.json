    "ATTACHMENT": {
      "description": "ECR-R Short Form for attachment style assessment",
      "responseScale": "agreement5",
      "dimensions": {
        "ANXIETY": "Fear of abandonment and rejection",
        "AVOIDANCE": "Discomfort with closeness and dependency"
      },
      "questions": [
        {"id": "ATT1", "text": "I worry about being abandoned", "dimension": "ANXIETY"},
        {"id": "ATT2", "text": "I worry a lot about my relationships", "dimension": "ANXIETY"},
        {"id": "ATT3", "text": "I worry that romantic partners won't care about me as much as I care about them", "dimension": "ANXIETY"},
        {"id": "ATT4", "text": "I need a lot of reassurance that I am loved by my partner", "dimension": "ANXIETY"},
        {"id": "ATT5", "text": "When I show my feelings for romantic partners, I'm afraid they will not feel the same about me", "dimension": "ANXIETY"},
        {"id": "ATT6", "text": "I am nervous when partners get too close to me", "dimension": "AVOIDANCE"},
        {"id": "ATT7", "text": "I prefer not to show a partner how I feel deep down", "dimension": "AVOIDANCE"},
        {"id": "ATT8", "text": "I find it difficult to allow myself to depend on romantic partners", "dimension": "AVOIDANCE"},
        {"id": "ATT9", "text": "I don't feel comfortable opening up to romantic partners", "dimension": "AVOIDANCE"}
      ],
      "scoring": {
        "SECURE": "Low anxiety + Low avoidance",
        "ANXIOUS_PREOCCUPIED": "High anxiety + Low avoidance",
        "DISMISSIVE_AVOIDANT": "Low anxiety + High avoidance",
        "FEARFUL_AVOIDANT": "High anxiety + High avoidance"
      }
    },
    "DEALBREAKER": {
      "description": "Binary questions that may result in automatic filtering",
      "responseScale": "binary",
      "warningLevel": "HIGH",
      "questions": [
        {"id": "DB1", "text": "Have you ever sent unsolicited intimate images to someone?", "redFlagIf": 2, "severity": "CRITICAL"},
        {"id": "DB2", "text": "Do you believe women should have fewer legal rights than men?", "redFlagIf": 2, "severity": "CRITICAL"},
        {"id": "DB3", "text": "Have you ever been physically aggressive with a romantic partner?", "redFlagIf": 2, "severity": "CRITICAL"},
        {"id": "DB4", "text": "Have you cheated in a committed relationship?", "redFlagIf": 2, "severity": "HIGH"},
        {"id": "DB5", "text": "Do you have children that you are not in contact with?", "redFlagIf": 2, "severity": "MODERATE"},
        {"id": "DB6", "text": "Have you ever been convicted of a violent crime?", "redFlagIf": 2, "severity": "CRITICAL"},
        {"id": "DB7", "text": "Do you believe your partner should not work outside the home?", "redFlagIf": 2, "severity": "HIGH"},
        {"id": "DB8", "text": "Have you ever deliberately damaged a partner's property during an argument?", "redFlagIf": 2, "severity": "CRITICAL"},
