    "RED_FLAG": {
      "description": "Indirect questions that reveal behavioral patterns",
      "responseScale": "freeText",
      "analysisRequired": true,
      "questions": [
        {
          "id": "RF1",
          "text": "How did your last relationship end?",
          "redFlags": ["always blames partner", "no self-reflection", "multiple similar endings"],
          "greenFlags": ["acknowledges own role", "learned lessons", "amicable ending"]
        },
        {
          "id": "RF2",
          "text": "What do you think your ex-partners would say about you?",
          "redFlags": ["all negative about exes", "victim mentality", "no positive acknowledgment"],
          "greenFlags": ["balanced view", "acknowledges faults", "growth oriented"]
        },
        {
          "id": "RF3",
          "text": "How do you handle it when someone doesn't respond to your messages quickly?",
          "redFlags": ["anger", "repeated messaging", "anxiety spiraling", "accusations"],
          "greenFlags": ["patience", "assumes positive intent", "gives space"]
        },
        {
          "id": "RF4",
          "text": "What makes you angry?",
          "redFlags": ["minor inconveniences", "others' emotions", "being questioned"],
          "greenFlags": ["injustice", "clear boundaries being violated", "measured response"]
        },
        {
          "id": "RF5",
          "text": "Describe a time you were wrong in an argument. What did you do?",
          "redFlags": ["can't think of one", "still justifies", "blames circumstances"],
          "greenFlags": ["admits fault", "apologized", "changed behavior"]
        },
        {
